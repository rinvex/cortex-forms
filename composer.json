{
    "name": "cortex/forms",
    "type": "cortex-module",
    "description": "Cortex Forms is a frontend layer for the dynamic form builder for Laravel, it's like the missing gem, the possibilities of using it are endless! With flexible API and powerful features you can build almost every kind of complex form with ease.",
    "keywords": [
        "form",
        "model",
        "rinvex",
        "laravel",
        "eloquent",
        "submission",
        "registration",
        "booking",
        "cortex",
        "module",
        "fields",
        "event",
        "forms"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-forms/issues",
        "source": "https://github.com/rinvex/cortex-forms",
        "docs": "https://github.com/rinvex/cortex-forms/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-forms/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "Illuminate/auth": "~5.6.0",
        "Illuminate/console": "~5.6.0",
        "Illuminate/http": "~5.6.0",
        "Illuminate/routing": "~5.6.0",
        "Illuminate/support": "~5.6.0",
        "cortex/foundation": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "^5.1.0",
        "laravelcollective/html": "~5.6.0",
        "league/fractal": "^0.17.0",
        "proengsoft/laravel-jsvalidation": "^2.2.0",
        "rinvex/composer": "dev-develop",
        "rinvex/forms": "dev-develop",
        "rinvex/menus": "dev-develop",
        "rinvex/support": "dev-develop",
        "silber/bouncer": "v1.0.0-rc.1",
        "spatie/laravel-activitylog": "^2.5.0",
        "yajra/laravel-datatables-buttons": "^3.2.0",
        "yajra/laravel-datatables-fractal": "^1.2.0",
        "yajra/laravel-datatables-html": "^3.7.0",
        "yajra/laravel-datatables-oracle": "^8.6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.19.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Forms\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Forms\\Providers\\FormsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
